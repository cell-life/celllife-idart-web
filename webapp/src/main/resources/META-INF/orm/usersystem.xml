<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <access>FIELD</access>

    <!-- User For System -->

    <entity class="org.celllife.idart.relationship.usersystem.UserSystem">
        <attributes>
            <id name="pk">
                <generated-value strategy="TABLE"/>
            </id>
            <basic name="relationship">
                <enumerated>STRING</enumerated>
            </basic>
            <embedded name="fromUser">
                <attribute-override name="value">
                    <column name="from_user"/>
                </attribute-override>
            </embedded>
            <embedded name="toSystem">
                <attribute-override name="value">
                    <column name="to_system"/>
                </attribute-override>
            </embedded>
        </attributes>
    </entity>

</entity-mappings>