<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 version="2.0">

    <access>FIELD</access>

    <!-- UnitOfMeasure -->

    <entity class="org.celllife.idart.domain.unitofmeasure.UnitOfMeasure">
        <attributes>
            <embedded-id name="code">
                <attribute-override name="value">
                    <column name="code"/>
                </attribute-override>
            </embedded-id>
            <basic name="name"/>
            <basic name="description"/>
            <embedded name="type">
                <attribute-override name="value">
                    <column name="type"/>
                </attribute-override>
            </embedded>
        </attributes>
    </entity>

</entity-mappings>