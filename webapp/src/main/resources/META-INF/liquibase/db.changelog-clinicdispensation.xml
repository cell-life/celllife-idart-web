<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <changeSet id="clinicdispensation-1" author="kevin.sewell">

        <createTable tableName="clinic_dispensation">
            <column name="pk" type="bigint">
                <constraints primaryKey="true"/>
            </column>
            <column name="clinic" type="varchar(31)"/>
            <column name="dispensation" type="varchar(31)"/>
            <column name="from_date" type="datetime"/>
            <column name="thru_date" type="datetime"/>
        </createTable>

    </changeSet>

</databaseChangeLog>
