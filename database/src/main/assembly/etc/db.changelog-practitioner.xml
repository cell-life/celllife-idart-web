<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <changeSet id="practitioner-1" author="kevin.sewell">

        <createTable tableName="practitioner">
            <column name="identifier" type="varchar(31)">
                <constraints primaryKey="true"/>
            </column>
            <column name="person_identifier" type="varchar(31)"/>
            <column name="person_type" type="varchar(31)"/>
            <column name="valid_from_date" type="DATE"/>
            <column name="valid_thru_date" type="DATE"/>
        </createTable>

    </changeSet>

</databaseChangeLog>
