<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <changeSet id="usersystem-1" author="kevin.sewell">

        <createTable tableName="user_system">
            <column name="pk" type="bigint">
                <constraints primaryKey="true"/>
            </column>
            <column name="relationship" type="varchar(31)"/>
            <column name="from_user" type="varchar(31)"/>
            <column name="to_system" type="varchar(31)"/>
        </createTable>

    </changeSet>

</databaseChangeLog>
